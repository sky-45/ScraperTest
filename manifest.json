{
    "name": "Scraper 2",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "./App/index.html"
      },
    "content_scripts":[
        {
            "matches":["https://www.linkedin.com/in/*"],
            "js":["App/Profile.js"]
        
        }
    ], 
    "background":{
        "service_worker":"App/background.js"
    }
  }



